function printParameter(a, b ,c){
    if (c == "+") {
        console.log(a + b)
    }
    else if (c == "-") {
        console.log(a - b)
    }
    else if (c == "*") {
        console.log(a * b)
    }
    else if (c == "/") {
        console.log(a / b)
    }
    else if (c == "**") {
        console.log(a ** b)
    }
    else if (c == "%") {
        console.log(a % b)
    }
    
}
