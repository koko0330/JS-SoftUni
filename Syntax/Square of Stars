function printParameter(input){
    let type = typeof(input)
    if (type == "number") {
        let num = Number(input)
        for (let index = 0; index < num; index++) {
            console.log("* ".repeat(num))            
        }
    } else {
        for (let index = 0; index < 5; index++) {
            console.log("* " * 5);
            
        }
    }

}
